{
  "name": "ngx-fabric-wrapper",
  "description": "Angular wrapper library for Fabric",
  "bugs": "https://github.com/zefoy/ngx-fabric-wrapper/issues",
  "version": "13.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "start": "ng serve app",
    "build": "ng build lib",
    "deploy": "deploy-to-git",
    "prepare": "ng build lib --configuration production",
    "publish": "npm publish ./dist/lib",
    "predeploy": "rimraf ./dist/app && mkdirp ./dist/app"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zefoy/ngx-fabric-wrapper.git"
  },
  "config": {
    "deployToGit": {
      "repository": "git@github.com:zefoy/ngx-fabric-wrapper.git",
      "branch": "gh-pages",
      "folder": "dist/app",
      "script": "ng build app --configuration production --base-href=ngx-fabric-wrapper --delete-output-path=false",
      "commit": "Publishing $npm_package_version",
      "user": {
        "name": "ZEF Devel",
        "email": "devel@zef.fi"
      }
    }
  },
  "dependencies": {
    "@angular/cdk": "^14.2.3",
    "@angular/common": "^14.2.3",
    "@angular/compiler": "^14.2.3",
    "@angular/core": "^14.2.3",
    "@angular/flex-layout": "14.0.0-beta.40",
    "@angular/platform-browser": "^14.2.3",
    "@angular/platform-browser-dynamic": "^14.2.3",
    "core-js": "^3.25.3",
    "fabric": "^5.2.4",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "^7.5.7",
    "stylelint": "^14.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.3",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "^14.2.3",
    "@angular/compiler-cli": "^14.2.3",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "codelyzer": "^6.0.2",
    "cpx": "^1.5.0",
    "deploy-to-git": "^0.4.1",
    "eslint": "^8.24.0",
    "mkdirp": "^1.0.4",
    "ng-packagr": "^14.2.1",
    "rimraf": "^3.0.2",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-order": "^5.0.0",
    "terser": "^5.15.0",
    "ts-node": "^10.9.1",
    "tsickle": "^0.46.3",
    "typescript": "~4.7.2",
    "watch": "^1.0.2",
    "zone.js": "^0.11.8"
  }
}
